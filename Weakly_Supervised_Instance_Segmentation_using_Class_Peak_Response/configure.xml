<configure>
    <preprocess>
        <img_size>448</img_size>
        <horizontal_flip>0.5</horizontal_flip>
        <vertical_flip>0.5</vertical_flip>
    </preprocess>

    <train>
        <loss>multilabel_soft_margin_loss</loss>
        <optimizer>sgd_optimizer</optimizer>
        <lr>0.01</lr>
        <momentum>0.9</momentum>
        <weight_decay>1.0e-4</weight_decay>
    </train>
</configure>